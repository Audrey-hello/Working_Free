  </li>
</ul>
<div class="tab-content" id="myTabContent">
  <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
  <div class="content-dashboard">
    <div class="cards-dashboard">
      <% @offices.each do |office| %>
        <div class= "card-dashboard">
          <%= link_to office_path(office) , class:'dashboard-hover' do %>
            <div class="card-dashboard">
              <% if office.photos.attached? %>
                <%= cl_image_tag office.photos.first.key, height: 300, width: 400, crop: :fill %>
                  <% else %>
                    <img src="https://images.unsplash.com/photo-1524758631624-e2822e304c36?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=750&q=80"/>
                    <%end%>
                      <div class="card-dashboard-infos">
                        <div>
                          <h2><%= office.title %></h2>
                          <hr>
                          <p><%= office.address %></p>
                          <p><%= office.description %></p>
                          <p><%= office.capacity%> Workstations</p>
                        </div>
                          <h2 class="card-office-pricing"> <%= office.price %>â‚¬</h2>
                      </div>
                    </div>
                  <% end %>
                  </div>
                <% end %>
              </div>
            </div>
          </div>
        <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">...</div>
      <div class="tab-pane fade" id="contact" role="tabpanel" aria-labelledby="contact-tab">...</div>
    </div>
